{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "plugins": [
    "react",
    "react-native",
    "jest",
    "flowtype"
  ],
  "extends": [
    "plugin:@wordpress/eslint-plugin/recommended",
    "plugin:flowtype/recommended"
  ],
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": true
    },
    "react": {
      "pragma": "React",
      "version": "16.6.1",
      "flowVersion": "0.67.1"
    },
  },
  "rules": {}
}
