{
	"__file": "wp_block",
	"title": "It's time",
	"content": "<!-- wp:group {\"align\":\"wide\",\"style\":{\"color\":{\"background\":\"#ffffff\"}}} -->\n<div class=\"wp-block-group alignwide has-background\" style=\"--wp--color--background:#ffffff\"><div class=\"wp-block-group__inner-container\"><!-- wp:columns {\"align\":\"full\"} -->\n<div class=\"wp-block-columns alignfull\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph {\"style\":{\"color\":{\"text\":\"#df3100\"},\"typography\":{\"lineHeight\":\"1.5\"}}} -->\n<p style=\"--wp--color--text:#df3100;--wp--typography--line-height:1.5\" class=\"has-text-color\"><strong>NEW</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"color\":{\"text\":\"#000000\"}}} -->\n<p style=\"--wp--color--text:#000000\" class=\"has-text-color\">John Lenwood \"Jackie\" McLean was an American jazz alto saxophonist, composer, bandleader, and educator, and is one of the few musicians to be elected to the DownBeat Hall of Fame in the year of their death.</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:spacer {\"height\":40} -->\n<div style=\"height:40px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:paragraph {\"style\":{\"color\":{\"text\":\"#000000\"}}} -->\n<p style=\"--wp--color--text:#000000\" class=\"has-text-color\">Derek Ansell's full-length biography of McLean, Sugar Free Saxophone (London: Northway Books, 2012), details the story of his career and provides a full analysis of his music on record.</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"width\":0} -->\n<div class=\"wp-block-column\" style=\"flex-basis:0%\"></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"width\":100} -->\n<div class=\"wp-block-column\" style=\"flex-basis:100%\"><!-- wp:paragraph {\"align\":\"center\",\"customFontSize\":250,\"style\":{\"color\":{\"text\":\"#000000\"},\"typography\":{\"lineHeight\":\"1\"}}} -->\n<p style=\"--wp--color--text:#000000;--wp--typography--line-height:1;font-size:250px\" class=\"has-text-align-center has-text-color\"><strong><em>it's time</em></strong> </p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph --></div></div>\n<!-- /wp:group -->"
  }
